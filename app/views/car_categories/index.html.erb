<h1>Categorias de carro</h1>

<%= link_to 'Voltar', root_path %>

<%= link_to 'Registrar uma nova categoria', new_car_category_path %>

<% @car_categories.each do |category| %>
  <h3><%= link_to category.name, category %></h3>
<% end %>

<% if @car_categories.empty? %>
  <h3>Nenhuma categoria cadastrada</h3>
<% end %>